<!DOCTYPE html>
<html>
<head>
<script rel="preload" as="script" src="https://cdn.jsdelivr.net/npm/redoc@next/bundles/redoc.standalone.js"></script>
  {{> head defaultPageTitle='Untitled'}}
</head>
<body class="redoc">
{{> header}}
<div id="redoc-container"></div>
<script>
Redoc.init('{{ page.attributes.specurl }}', {
  scrollYOffset: "header > .navbar > .container-fluid",
  nativeScrollbars: true,
  noAutoAuth: true,
  theme: {
    sidebar: {
      backgroundColor: '#04485b',
      textColor: 'white',
      width: '23.75rem',
    },
    colors: {
      primary: {
        main: '#fbfbfb'
      },
      text: {
        primary: '#fbfbfb',
      },
      success: {
        main: '#12c924'
      },
      error: {
        main: '#ff0400'
      },
    },
    typography: {
      fontSize: '18px',
      lineHeight: '1.5em',
      fontWeightRegular: '400',
      fontWeightBold: '700',
      fontWeightLight: '300',
      fontFamily: 'Source Sans Pro, sans-serif',
      smoothing: 'antialiased',
      optimizeSpeed: true,
      headings: {
        fontFamily: 'Graublau Sans, sans-serif',
        fontWeight: '700',
        lineHeight: '1.6em',
      },
      code: {
        fontFamily: 'Source Code Pro',
        fontWeight: '500',
        fontSize: '16px',
      }
    },
  }
}, document.getElementById('redoc-container'))
</script>
{{> body-scripts}}
</body>
</html>
