<script src="https://cdn.jsdelivr.net/npm/@docsearch/js@alpha"></script>
<script>
  (function () {
    window.process = {
      env: {
        NODE_ENV: 'production'
      }
    }
    var interface_version = "{{page.componentVersion.displayVersion}}"
    var interface_version_list = [
      'stable7',
      'early',
      'beta7'
    ]
    if (!interface_version_list.includes(interface_version)) {
      interface_version = interface_version_list[0]
    }

    var plugin_version = "{{page.componentVersion.displayVersion}}"
    var plugin_version_list = [
      '5.0.0',
      '4.6.0'
    ]
    if (!plugin_version_list.includes(plugin_version)) {
      plugin_version = plugin_version_list[0]
    }

    docsearch({
      appId: '6PTARL4KJP',
      apiKey: 'e7e08c62fb27474b5104e29e3e673645',
      indexName: 'plentymarkets_beta_doc',
      container: '#docsearch',

      searchParameters: {
        hitsPerPage: 10,
      },
    })
  })()
</script>

